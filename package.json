{
  "name": "stripe-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@ant-design/charts": "^1.4.1",
    "@ant-design/icons": "^4.7.0",
    "@metamodules/mongo": "^1.1.1",
    "antd": "^4.21.5",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "comma-number": "^2.1.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "currency-symbol-map": "^5.1.0",
    "deploy-node-app": "^1.4.3",
    "dotenv": "^8.0.0",
    "express-respond": "^1.6.1",
    "express-response-formatter": "^2.0.0",
    "express-session": "^1.17.3",
    "human-date": "^1.4.0",
    "moment": "^2.29.4",
    "mongoose": "^4.13.21",
    "node-mongoose-auth": "^1.0.4",
    "ordinal": "^1.0.3",
    "patch-package": "^6.4.7",
    "random-gradient": "^0.0.2",
    "react": "^16.8.6",
    "react-auth-kit": "^2.1.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^6.3.0",
    "react-scripts": "3.0.1",
    "react-wavify": "^1.6.1",
    "stripe": "^9.12.0",
    "user-permissions": "^0.4.4"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "concurrently": "^4.1.0",
    "cross-spawn": "^6.0.5",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1"
  },
  "scripts": {
    "start": "bash bin/dev_api.sh",
    "dev": "bash bin/dev.sh",
    "build": "react-scripts build",
    "deploy": "deploy-node-app",
    "test": "react-scripts test",
    "eject-www": "react-scripts eject",
    "psql": "export $(cat .env | xargs) && PGPASSWORD=${POSTGRES_PASSWORD} psql -h ${POSTGRES_SERVICE_HOST} -p ${POSTGRES_SERVICE_PORT} -U ${POSTGRES_USER} ${POSTGRES_DB}",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "deploy-node-app": {
    "prod": {
      "port": 4000,
      "protocol": "http",
      "entrypoint": "src/api/index.js",
      "type": "spa"
    },
    "dev": {
      "port": 4000,
      "protocol": "http",
      "entrypoint": "src/api/index.js",
      "registry": "",
      "type": "spa",
      "isPublic": false
    }
  },
  "proxy": "http://localhost:4000/"
}
